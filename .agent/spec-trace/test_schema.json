{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "title": "Test Level Traceability Registry",
  "x-template": "test_template.json",
  "description": "Registry for test level traceability IDs",
  "properties": {
    "version": {
      "type": "string",
      "description": "Schema version",
      "default": "1.0.0"
    },
    "description": {
      "type": "string",
      "description": "Registry description",
      "default": "Test level traceability IDs"
    },
    "test": {
      "type": "array",
      "description": "Test level traceability IDs",
      "x-frontmatter-part": true,
      "items": { "$ref": "traceability_test_schema.json" }
    }
  },
  "required": ["version", "description", "test"],
  "additionalProperties": false
}
