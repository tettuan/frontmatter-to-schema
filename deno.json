{
  "name": "@scope/frontmatter-to-schema",
  "version": "0.1.0",
  "description": "Convert frontmatter to JSON schema",
  "license": "MIT",
  "exports": "./mod.ts",
  "exclude": [
    "tests/e2e/test-output/"
  ],
  "tasks": {
    "ci": "deno run --allow-read --allow-write --allow-run --allow-env jsr:@aidevtool/ci",
    "ci:dirty": "deno run --allow-read --allow-write --allow-run --allow-env jsr:@aidevtool/ci --allow-dirty",
    "dev": "deno run --allow-all --watch src/main.ts",
    "start": "deno run --allow-all src/main.ts",
    "test": "deno test --allow-all tests/",
    "frontmatter-to-schema": "deno run --allow-read --allow-write --allow-env src/cli.ts",
    "fm2s": "deno run --allow-read --allow-write --allow-env src/cli.ts",
    "example:climpt": "deno run --allow-all src/main.ts --config examples/climpt-registry/config.json",
    "example:articles": "deno run --allow-all src/main.ts --config examples/articles-index/config.json",
    "example:new-cli": "deno run --allow-read --allow-write --allow-env src/cli.ts schemas/registry_schema.json output/registry.json '.agent/climpt/prompts/**/*.md' --verbose"
  },
  "imports": {
    "@anthropic-ai/claude-code": "npm:@anthropic-ai/claude-code@^1.0.89",
    "@std/assert": "jsr:@std/assert@^1.0.14",
    "@std/testing": "jsr:@std/testing@^1.0.15",
    "jsr:@std/fs": "jsr:@std/fs@1.0.8",
    "jsr:@std/front-matter": "jsr:@std/front-matter@1.0.5",
    "jsr:@std/cli": "jsr:@std/cli@1.0.9"
  }
}
