{
  "version": "1.0.0",
  "description": "Basic command registry example",
  "tools": {
    "availableConfigs": [
      "git",
      "spec"
    ],
    "commands": [
      {
        "c1": "git",
        "c2": "create",
        "c3": "branch",
        "title": "Create Git Branch",
        "description": "Create a new git branch for feature development",
        "usage": "git checkout -b feature/new-feature",
        "options": {
          "input": [
            "string"
          ],
          "adaptation": [
            "default"
          ],
          "input_file": [
            false
          ],
          "stdin": [
            false
          ],
          "destination": [
            false
          ]
        }
      },
      {
        "c1": "spec",
        "c2": "analyze",
        "c3": "quality-metrics",
        "title": "Analyze Quality Metrics",
        "description": "Analyze code quality metrics from specifications",
        "usage": "climpt spec analyze quality-metrics --input=spec.md",
        "options": {
          "input": [
            "file",
            "stdin"
          ],
          "adaptation": [
            "default",
            "detailed"
          ],
          "input_file": [
            true
          ],
          "stdin": [
            true
          ],
          "destination": [
            true,
            false
          ]
        }
      },
      {
        "c1": "",
        "c2": "",
        "c3": "",
        "title": "",
        "description": "",
        "usage": "",
        "options": {
          "input": "",
          "adaptation": "",
          "input_file": "",
          "stdin": "",
          "destination": ""
        }
      }
    ]
  }
}
